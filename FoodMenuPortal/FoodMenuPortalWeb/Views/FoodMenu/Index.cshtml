@model FoodMenuPortalWeb.ViewModels.FoodMenuListViewModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        @{  for (int i = 0; i < Model.FoodMenus.Count; i++)
				{
                <div>@Html.ActionLink(Model.FoodMenus[i].Name, "Id", "FoodMenu", new { id = Model.FoodMenus[i].Id }, null)</div>
				}
        }
        <div>@Html.ActionLink("Add New FoodMenu", "Create")</div>
    </div>
</body>
</html>
